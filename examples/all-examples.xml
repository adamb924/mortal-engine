            <should-be-accepted>yes</should-be-accepted>
        </recognition-test>
        <message>(Obviously) Infinitive suffixes don't go on nouns</message>
        <recognition-test>
            <input lang="wk-LA">doninf</input>
            <should-be-accepted>no</should-be-accepted>
        </recognition-test>
    </schema>
    <schema label="Phonological Allomorphy">
        <morphology-file>12-Phonological-Allomorphy.xml</morphology-file>
        <message>Classifiers are obligatory (i.e., no bare stems)</message>
        <recognition-test>
            <input lang="wk-LA">sɪn</input>
            <should-be-accepted>no</should-be-accepted>
        </recognition-test>
        <message>Words with ATR vowels get an ATR classifier</message>
        <recognition-test>
            <input lang="wk-LA">esit</input>
            <should-be-accepted>yes</should-be-accepted>
        </recognition-test>
        <recognition-test>
            <input lang="wk-LA">eben</input>
            <should-be-accepted>yes</should-be-accepted>
        </recognition-test>
        <recognition-test>
            <input lang="wk-LA">emot</input>
            <should-be-accepted>yes</should-be-accepted>
        </recognition-test>
        <message>Words with ATR vowels can't take a non-ATR classifier</message>
        <recognition-test>
            <input lang="wk-LA">ɛsit</input>
            <should-be-accepted>no</should-be-accepted>
        </recognition-test>
        <recognition-test>
            <input lang="wk-LA">ɛben</input>
            <should-be-accepted>no</should-be-accepted>
        </recognition-test>
        <recognition-test>
            <input lang="wk-LA">ɛmot</input>
            <should-be-accepted>no</should-be-accepted>
        </recognition-test>
        <message>Non-ATR words take a non-ATR classifier</message>
        <recognition-test>
            <input lang="wk-LA">ɛsɪn</input>
            <should-be-accepted>yes</should-be-accepted>
        </recognition-test>
        <recognition-test>
            <input lang="wk-LA">ɛkɛm</input>
            <should-be-accepted>yes</should-be-accepted>
        </recognition-test>
        <recognition-test>
            <input lang="wk-LA">ɛkɔŋ</input>
            <should-be-accepted>yes</should-be-accepted>
        </recognition-test>
        <message>Non-ATR words cannot take an ATR classifier</message>
        <recognition-test>
            <input lang="wk-LA">esɪn</input>
            <should-be-accepted>no</should-be-accepted>
        </recognition-test>
        <recognition-test>
            <input lang="wk-LA">ekɛm</input>
            <should-be-accepted>no</should-be-accepted>
        </recognition-test>
        <recognition-test>
            <input lang="wk-LA">ekɔŋ</input>
            <should-be-accepted>no</should-be-accepted>
        </recognition-test>    
    </schema>
